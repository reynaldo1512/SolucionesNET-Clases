@using DAO
@model tbExamen
@{
    ViewBag.Title = "Nuevo";
}
<h2>
    <a style="float:right;color:green" href="@Url.Action("Index","TipoExamen")"><i class="fas fa-chevron-circle-left"></i> </a>Nuevo tipo de examen
</h2>
<hr />

@using (Html.BeginForm("Nuevo", "TipoExamen", FormMethod.Post))
{

    <div class="form-group">
        <label>Nombre Examen </label>
        @Html.TextBoxFor(m => m.nombreExamen, new { @class = "form-control", @required = "required" })

    </div>
    <div class="form-group">
        <label> Precio </label>
        @Html.TextBoxFor(m => m.precioUnidad, new { @class = "form-control", @required = "required" })

    </div>
    <div class="form-group">
        <label> Estado </label>
        @Html.TextBoxFor(m => m.estado, new { @class = "form-control", @required = "required" })

    </div>





    <div class="form-group">
        <input type="submit" value="Guardar" class="btn btn-success" />

    </div>


}


